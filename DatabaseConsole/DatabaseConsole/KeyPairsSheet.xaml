<UserControl x:Class="DatabaseConsole.KeyPairsSheet"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DatabaseConsole"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="28"/>
        </Grid.RowDefinitions>
        <Button x:Name="mAddBtn" Content="添加" HorizontalAlignment="Center" Height="24" Margin="0" Grid.Row="1" VerticalAlignment="Center" Width="50" FontSize="14" Click="mAddBtn_Click"/>
        <ScrollViewer  Margin="0">
            <StackPanel x:Name="mPairsBox"/>
        </ScrollViewer>

    </Grid>
</UserControl>
